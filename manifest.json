{
    "manifest_version": 2,
    "name": "Jamra Somoim",
    "description": "Jamra Somoim Extension",
    "version": "1.2",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
     "js": ["js/jquery.min.js", "js/bootstrap.min.js", "js/app.js"],
     "matches": ["http://*/*", "https://*/*"]
   }]
}